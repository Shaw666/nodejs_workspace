<!DOCTYPE html><html lang=en>{{> header}}{{> page_header}}<div id=body-container><div id=body-content>{{> nav}}<section class="page container"><form class=form-horizontal><div class=container><div class=row><div id=server-settings-row class=span10><fieldset><legend>系统日志</legend><br><div class="box pattern-sandstone span13"><div class=box-header><i class=icon-table></i><h5>系统日志(最新100条)</h5></div><div class="box-content box-table"><table class="table table-striped table-hover table-bordered tablesorter"><thead><tr><th class="center span1">序号<th class="center span3">时间<th class="center span1">帐号<th class="center span2">ip<th class="center span1">端口<th class="center span7">内容<tbody id=system-log></table></div></div></fieldset></div></div></div></form></section></div></div>{{> footer}}<script src=../js/jquery/jquery-tablesorter.js></script><script src=../js/jquery/jquery-chosen.js></script><script>$(document).ready(function(){$("#session-list").tablesorter(),$.getJSON("/api/get-sys-log").done(function(p){var e=$("#system-log");e.empty();for(var t=0;t<p.length;t++)if(p[t]){var d=p[t].split(";"),n=$("<tr>");n.append($("<td>").append(t)),n.append($("<td>").append(d.shift())),n.append($("<td>").append(d.shift())),n.append($("<td>").append(d.shift())),n.append($("<td>").append(d.shift())),n.append($("<td>").append(d.join(";"))),n.appendTo(e)}})})</script>